class Solution {
    public ListNode deleteDuplicates(ListNode head) {
        if(head==null)return head;
        ListNode current=head.next;
        ListNode prev=head;
        while(current!=null){    
            if(prev.val==current.val){
                prev.next=current.next;
                current=current.next;
            } else{
             current=current.next;
             prev=prev.next; 
            }
        }
        return head;
    }
}
