class Solution {
    public ListNode reverseList(ListNode head) {
        if(head==null||head.next==null)return head;
        ListNode prev=null; ListNode current=head; ListNode next=head.next;
        while(current!=null&&current.next!=null){
            current.next=prev;
            prev=current;
            current=next;
            next=next.next;
        }
        current.next=prev;
        return current;
    }
}
