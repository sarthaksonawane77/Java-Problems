public class Solution {
    public ListNode detectCycle(ListNode head) {
        if(head==null||head.next==null)return null;
        ListNode slow=head; ListNode fast=head; int pos=0;
        while(fast!=null&&fast.next!=null){
            slow=slow.next;
            fast=fast.next.next;
            if(slow==fast){
                slow=head;
                while(fast!=slow){
                    fast=fast.next;
                    slow=slow.next;
                    pos++;
                }
                return slow;
            }
        }
        return null;
    }
}
