public class arrayOperations{
    public static void main(String[] args) {
        int[] arr={1,1,1,2,2,5,5,5,5,5};
        int j=0;
        for(int i =1;i<arr.length;i++){
            if(arr[i]!=arr[i-1]) {
                arr[j]= arr[i];
                j++;
            }
        }
        for (int i=0;i<j+1;i++) {
            System.out.println(arr[i]);
        }
        }
    }
